{
  "name": "stratz.js",
  "version": "1.0.0",
  "description": "A Node.js STRATZ REST API wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run docs",
    "clean": "rm -rf lib && rm DOCS.md",
